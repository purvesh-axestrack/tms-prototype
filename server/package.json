{
  "name": "tms-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run",
